{
  "name": "stoobz-stack-monorepo",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "cd server && npm run start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
