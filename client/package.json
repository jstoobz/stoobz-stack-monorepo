{
  "name": "stoobz-stack-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./config/webpack.config.js --mode production --env.production",
    "start": "nodemon --watch ./config/webpack.config.js --exec \"NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js --mode development --color --env.development\"",
    "build_latestt": "NODE_ENV=production webpack --config ./config/webpack.prod.config.js --mode production --env.production",
    "start_latest": "nodemon --watch ./config/webpack.base.config.js --exec \"NODE_ENV=development webpack-dev-server --config ./config/webpack.base.config.js --mode development --color --env.development\"",
    "build_latest": "rm -rf ./dist/ && NODE_ENV=production webpack --config ./config/webpack.prod.config.js --mode production",
    "start_older": "NODE_ENV=development webpack-dev-server --mode development --config ./config/webpack.dev.config.js --env.PLATFORM=local",
    "start_old": "NODE_ENV='development' && webpack-dev-server --mode development --config config/webpack.config.js",
    "build_old": "NODE_ENV='production' && webpack --mode production --config config/webpack.config.js"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
